$RELEASE_GRAPENLP --desc-parser -sxwct -pbreadth-first -elrb-tree-3w -bstd -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -sxwc -pbreadth-first -elrb-tree-3w -blrb-tree -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -sxwct -pbreadth-first -elrb-tree-3w -blrb-tree -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -sxwc -pbreadth-first -elrb-tree-3w -blrb-tree-3w -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -sxwct -pbreadth-first -elrb-tree-3w -blrb-tree-3w -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT

$RELEASE_GRAPENLP --desc-parser -sxwc -pdepth-first -bstd -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -sxwct -pdepth-first -bstd -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -sxwc -pdepth-first -blrb-tree -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -sxwct -pdepth-first -blrb-tree -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -sxwc -pdepth-first -blrb-tree-3w -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -sxwct -pdepth-first -blrb-tree-3w -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT

$RELEASE_GRAPENLP --desc-parser -snxwc -pbreadth-first -estd -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS_NO_OUTPUT $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -snxwct -pbreadth-first -estd -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS_NO_OUTPUT $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -snxwc -pbreadth-first -elrb-tree -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS_NO_OUTPUT $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -snxwct -pbreadth-first -elrb-tree -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS_NO_OUTPUT $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -snxwc -pbreadth-first -elrb-tree-3w -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS_NO_OUTPUT $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -snxwct -pbreadth-first -elrb-tree-3w -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS_NO_OUTPUT $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT

$RELEASE_GRAPENLP --desc-parser -snxwc -pdepth-first -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS_NO_OUTPUT $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -snxwct -pdepth-first -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS_NO_OUTPUT $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
