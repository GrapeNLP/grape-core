$RELEASE_GRAPENLP --desc-parser -sxwct -pbreadth-first -estd -blrb-tree-3w -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -sxwc -pbreadth-first -elrb-tree -bstd -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -sxwct -pbreadth-first -elrb-tree -bstd -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -sxwc -pbreadth-first -elrb-tree -blrb-tree -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -sxwct -pbreadth-first -elrb-tree -blrb-tree -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -sxwc -pbreadth-first -elrb-tree -blrb-tree-3w -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
$RELEASE_GRAPENLP --desc-parser -sxwct -pbreadth-first -elrb-tree -blrb-tree-3w -r$RARG $INPUT_GRAMMAR $COMPRESSED_DICO $MY_CORPUS $TEST_CONTEXT $OUTPUT | tee -a $MY_STATS_TXT
