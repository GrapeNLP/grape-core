#!/bin/bash

GRAPE_FOLDER=`pwd`/..
THESIS_FOLDER=$GRAPE_FOLDER/../../papers/thesis_sastre
BENCHMARKS_FOLDER=$THESIS_FOLDER/benchmarks
SCRIPT_FOLDER=$GRAPE_FOLDER/scripts
A_INPUT=$GRAPE_FOLDER/a.txt
TEST_INPUT=$GRAPE_FOLDER/test.txt
CORPUS=$GRAPE_FOLDER/corpus.txt
CORPUS_NO_OUTPUT=$GRAPE_FOLDER/corpus_no_output.txt
CORPUS_NO_KEYWORD=$GRAPE_FOLDER/corpus_no_keyword.txt
CORPUS_NO_OUTPUT_NO_KEYWORD=$GRAPE_FOLDER/corpus_no_output_no_keyword.txt
OUTPUT=$GRAPE_FOLDER/result.txt
VALIDATE=$GRAPE_FOLDER/validate.txt
FILESTOCLEAR="$OUTPUT $VALIDATE"

DOTGRAMMAR=$GRAPE_FOLDER/grammar.dot
DOTDIRTYPARSES=$GRAPE_FOLDER/dirty_parses.dot
DOTPARSES=$GRAPE_FOLDER/parses.dot
DOTINVPARSES=$GRAPE_FOLDER/inv_parses.dot
DOTRBTREE=$GRAPE_FOLDER/rb_tree.dot
DOTRBTREEDELETED=$GRAPE_FOLDER/rb_tree_deleted.dot
DOTLRBTREE=$GRAPE_FOLDER/lrb_tree.dot
DOTLRBTREEDELETED=$GRAPE_FOLDER/lrb_tree_deleted.dot
FILESTOCLEAR="$FILESTOCLEAR $DOTGRAMMAR $DOTDIRTYPARSES $DOTPARSES $DOTINVPARSES $DOTRBTREE $DOTRBTREEDELETED $DOTLRBTREE $DOTLRBTREEDELETED"

JPGGRAMMAR=$GRAPE_FOLDER/grammar.jpg
JPGDIRTYPARSES=$GRAPE_FOLDER/dirty_parses.jpg
JPGPARSES=$GRAPE_FOLDER/parses.jpg
JPGINVPARSES=$GRAPE_FOLDER/inv_parses.jpg
FILESTOCLEAR="$FILESTOCLEAR $JPGGRAMMAR $JPGDIRTYPARSES $JPGPARSES $JPGINVPARSES"

SVGGRAMMAR=$GRAPE_FOLDER/grammar.svg
SVGDIRTYPARSES=$GRAPE_FOLDER/dirty_parses.svg
SVGPARSES=$GRAPE_FOLDER/parses.svg
SVGINVPARSES=$GRAPE_FOLDER/inv_parses.svg
SVGRBTREE=$GRAPE_FOLDER/rb_tree.svg
SVGRBTREEDELETED=$GRAPE_FOLDER/rb_tree_deleted.svg
SVGLRBTREE=$GRAPE_FOLDER/lrb_tree.svg
SVGLRBTREEDELETED=$GRAPE_FOLDER/lrb_tree_deleted.svg
FILESTOCLEAR="$FILESTOCLEAR $SVGGRAMMAR $SVGDIRTYPARSES $SVGPARSES $SVGINVPARSES $SVGRBTREE $SVGRBTREEDELETED $SVGLRBTREE $SVGLRBTREEDELETED"
 
DATA_FOLDER=$GRAPE_FOLDER/Data
UNITEX_DATA_FOLDER=$DATA_FOLDER/Unitex/Spanish
DICO_FOLDER=$UNITEX_DATA_FOLDER/Dela
TEXT_DICO=$DICO_FOLDER/delaf.dic
TEXT_MINI_DICO=$DICO_FOLDER/delaf_mini.dic
COMPRESSED_DICO=$DICO_FOLDER/delaf_norm.bin
COMPRESSED_MINI_DICO=$DICO_FOLDER/delaf_mini.bin

GRAMMAR_FOLDER=$UNITEX_DATA_FOLDER/Graphs
TID_GRAMMAR=$GRAMMAR_FOLDER/axiom.fst2
TID_NO_KEYWORD_GRAMMAR=$GRAMMAR_FOLDER/axiom_no_keyword.fst2
TID_FST_NO_KEYWORD_GRAMMAR=$GRAMMAR_FOLDER/axiom_fst_no_keyword.fst2
TEST_GRAMMAR=$GRAMMAR_FOLDER/test.fst2
AEEE_GRAMMAR=$GRAPE_FOLDER/aeee.fst2
WAEEE_GRAMMAR=$GRAPE_FOLDER/waeee.fst2
ANBN_GRAMMAR=$GRAPE_FOLDER/anbn.fst2
WANBN_GRAMMAR=$GRAPE_FOLDER/wanbn.fst2
DICO_TEST_GRAMMAR=$GRAMMAR_FOLDER/dico_test.fst2

MANAGER_FOLDER=$GRAPE_FOLDER/manager

BIN_FOLDER=$GRAPE_FOLDER/bin
DEBUG_FOLDER=$BIN_FOLDER/debug
RELEASE_FOLDER=$BIN_FOLDER/release
MTRACE_FOLDER=$BIN_FOLDER/mtrace
PROFILE_FOLDER=$BIN_FOLDER/profile
MANAGER_DEBUG_FOLDER=$DEBUG_FOLDER/cgrape
MANAGER_RELEASE_FOLDER=$RELEASE_FOLDER/cgrape
MANAGER_MTRACE_FOLDER=$MTRACE_FOLDER/cgrape
MANAGER_PROFILE_FOLDER=$PROFILE_FOLDER/cgrape

RELEASE_NLP=$MANAGER_RELEASE_FOLDER/nlp
DEBUG_NLP=$MANAGER_DEBUG_FOLDER/nlp
MTRACE_NLP=$MANAGER_MTRACE_FOLDER/nlp
PROFILE_NLP=$MANAGER_PROFILE_FOLDER/nlp

SIMPLIFY_TEMPLATES=$RELEASE_FOLDER/simplify_templates/simplify_templates
LRB_TREE_EFFICIENCY=$PROFILE_FOLDER/lrb_tree/lrb_tree_efficiency
LATEX_HBAR_CHART=$RELEASE_FOLDER/latex_hbar_chart/latex_hbar_chart

STATS_TXT=$BENCHMARKS_FOLDER/stats.txt
STATS_NO_KEYWORD_TXT=$BENCHMARKS_FOLDER/stats_no_keyword.txt
STATS_FST_NO_KEYWORD_TXT=$BENCHMARKS_FOLDER/stats_fst_no_keyword.txt
STATS_TEX=$BENCHMARKS_FOLDER/stats.tex
STATS_NO_KEYWORD_TEX=$BENCHMARKS_FOLDER/stats_no_keyword.tex
STATS_FST_NO_KEYWORD_TEX=$BENCHMARKS_FOLDER/stats_fst_no_keyword.tex
FILESTOCLEAR="$FILESTOCLEAR $STATS_TXT $STATS_TEX $STATS_NO_KEYWORD_TXT $STATS_NO_KEYWORD_TEX $STATS_FST_NO_KEYWORD_TXT $STATS_FST_NO_KEYWORD_TEX"

SPEEDUP_CALCULATOR=$RELEASE_FOLDER/speedup_calculator/speedup_calculator

MALLOC_TRACE=$SCRIPT_FOLDER/mtrace.txt
export MALLOC_TRACE
LEAK_FOLDER=$SCRIPT_FOLDER/mleak
LEAK_TRACE=$LEAK_FOLDER/leak.txt
FILESTOCLEAR="$FILESTOCLEAR $MALLOC_TRACE $LEAK_TRACE"

RAW_PROFILE_TRACE=$SCRIPT_FOLDER/gmon.out
TOP_DOWN_PROFILE_TRACE=$SCRIPT_FOLDER/top_down_prof.txt
BREADTH_FIRST_PROFILE_TRACE=$SCRIPT_FOLDER/breadth_first_prof.txt
EARLEY_PROFILE_TRACE=$SCRIPT_FOLDER/earley_prof.txt
TO_FPRTN_BF_PROFILE_TRACE=$SCRIPT_FOLDER/to_fprtn_bf_prof.txt
FILESTOCLEAR="$FILESTOCLEAR $RAW_PROFILE_TRACE $TOP_DOWN_PROFILE_TRACE $BREADTH_FIRST_PROFILE_TRACE $EARLEY_PROFILE_TRACE $TO_FPRTN_BF_PROFILE_TRACE"
