#!/bin/bash
cd `dirname $0`
. paths

if [ $# -gt 0 ]
then
$PROFILE_GRAPENLP -sxcw -pdepth-first -r$1 $CHATBOT_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT
else
$PROFILE_GRAPENLP -sxcw -pdepth-first $CHATBOT_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT
fi
echo \#\#\#\#\#\#\#\#\#\#\#\# > $TOP_DOWN_PROFILE_TRACE
echo \# Top-Down \# >> $TOP_DOWN_PROFILE_TRACE
echo \#\#\#\#\#\#\#\#\#\#\#\# >> $TOP_DOWN_PROFILE_TRACE
echo >> $TOP_DOWN_PROFILE_TRACE
gprof $PROFILE_GRAPENLP | $SIMPLIFY_TEMPLATES >> $TOP_DOWN_PROFILE_TRACE
vi $TOP_DOWN_PROFILE_TRACE
