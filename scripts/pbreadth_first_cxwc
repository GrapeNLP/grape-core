#!/bin/bash
cd `dirname $0`
. paths

if [ $# -gt 0 ]
then
$PROFILE_GRAPENLP -sxcw -pbreadth-first -r$1 $CHATBOT_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT
else
$PROFILE_GRAPENLP -sxcw -pbreadth-first $CHATBOT_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT
fi
echo \#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\# > $BREADTH_FIRST_PROFILE_TRACE
echo \# Breadth-First \# >> $BREADTH_FIRST_PROFILE_TRACE
echo \#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\# >> $BREADTH_FIRST_PROFILE_TRACE
echo >> $BREADTH_FIRST_PROFILE_TRACE
gprof $PROFILE_GRAPENLP | $SIMPLIFY_TEMPLATES >> $BREADTH_FIRST_PROFILE_TRACE
vi $BREADTH_FIRST_PROFILE_TRACE
