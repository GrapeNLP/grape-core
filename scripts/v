$RELEASE_GRAPENLP --desc-parser -xwc -pto-fprtn-and-top-blackboard-extract -elrb-tree-3w -bstd $CHATBOT_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT $VALIDATE | grep -E "\*\ |Validation|Invalid|fault" | sed 's/.*\*\ //g' | sed 's/\ \*.*//g'
$RELEASE_GRAPENLP --desc-parser -xwc -pto-fprtn-and-top-blackboard-extract -elrb-tree-3w -blrb-tree $CHATBOT_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT $VALIDATE | grep -E "\*\ |Validation|Invalid|fault" | sed 's/.*\*\ //g' | sed 's/\ \*.*//g'
$RELEASE_GRAPENLP --desc-parser -xwc -pto-fprtn-and-top-blackboard-extract -elrb-tree-3w -blrb-tree-3w $CHATBOT_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT $VALIDATE | grep -E "\*\ |Validation|Invalid|fault" | sed 's/.*\*\ //g' | sed 's/\ \*.*//g'

