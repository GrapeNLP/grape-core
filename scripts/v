#!/bin/bash
cd `dirname $0`
. paths

#To-FPRTN & top blackboard extraction
$RELEASE_GRAPENLP --desc-parser -xwc -pto-fprtn-and-top-blackboard-extract -estd -bstd $CHATBOT_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT $VALIDATE | grep -E "\*\ |Validation|Invalid|fault" | sed 's/.*\*\ //g' | sed 's/\ \*.*//g'
$RELEASE_GRAPENLP --desc-parser -xwc -pto-fprtn-and-top-blackboard-extract -estd -blrb-tree $CHATBOT_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT $VALIDATE | grep -E "\*\ |Validation|Invalid|fault" | sed 's/.*\*\ //g' | sed 's/\ \*.*//g'
$RELEASE_GRAPENLP --desc-parser -xwc -pto-fprtn-and-top-blackboard-extract -estd -blrb-tree-3w $CHATBOT_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT $VALIDATE | grep -E "\*\ |Validation|Invalid|fault" | sed 's/.*\*\ //g' | sed 's/\ \*.*//g'
$RELEASE_GRAPENLP --desc-parser -xwc -pto-fprtn-and-top-blackboard-extract -elrb-tree -bstd $CHATBOT_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT $VALIDATE | grep -E "\*\ |Validation|Invalid|fault" | sed 's/.*\*\ //g' | sed 's/\ \*.*//g'
$RELEASE_GRAPENLP --desc-parser -xwc -pto-fprtn-and-top-blackboard-extract -elrb-tree -blrb-tree $CHATBOT_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT $VALIDATE | grep -E "\*\ |Validation|Invalid|fault" | sed 's/.*\*\ //g' | sed 's/\ \*.*//g'
