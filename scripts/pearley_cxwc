cd `dirname $0`
. paths

#!/bin/bash
cd `dirname $0`
. ./paths

if [ $# -gt 0 ]
then
$PROFILE_GRAPENLP -sxcw -pearley -r$1 $CHATBOT_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT
else
$PROFILE_GRAPENLP -sxcw -pearley $CHATBOT_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT
fi
echo \#\#\#\#\#\#\#\#\#\# > $EARLEY_PROFILE_TRACE
echo \# Earley \# >> $EARLEY_PROFILE_TRACE
echo \#\#\#\#\#\#\#\#\#\# >> $EARLEY_PROFILE_TRACE
echo >> $EARLEY_PROFILE_TRACE
gprof $PROFILE_GRAPENLP | $SIMPLIFY_TEMPLATES >> $EARLEY_PROFILE_TRACE
vi $EARLEY_PROFILE_TRACE
