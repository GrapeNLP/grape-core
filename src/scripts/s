#!/bin/bash
cd `dirname $0`
. paths

RARG="1s"

$RELEASE_NLP --desc-parser -sxwc -pto-fprtn-zpps -estd -r$RARG $TID_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT | grep -E '\*\ |sentences per second' | sed 's/Average sentences per second://g' | sed 's/\*\ //g' | sed 's/\ \*//g'
$RELEASE_NLP --desc-parser -sxwc -pto-fprtn-zpps -elrb-tree -r$RARG $TID_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT | grep -E '\*\ |sentences per second' | sed 's/Average sentences per second://g' | sed 's/\*\ //g' | sed 's/\ \*//g'
$RELEASE_NLP --desc-parser -sxwc -pto-fprtn-zpps -elrb-tree-3w -r$RARG $TID_GRAMMAR $COMPRESSED_DICO $CORPUS $OUTPUT | grep -E '\*\ |sentences per second' | sed 's/Average sentences per second://g' | sed 's/\*\ //g' | sed 's/\ \*//g'

